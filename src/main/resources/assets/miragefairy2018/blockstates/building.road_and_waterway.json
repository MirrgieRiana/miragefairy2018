{
	"multipart": [

		{
			"apply": { "model": "miragefairy2018:building.road.center" }
		},
		{
			"when": { "road_north": "true" },
			"apply": { "model": "miragefairy2018:building.road.side", "y": 0 }
		},
		{
			"when": { "road_east": "true" },
			"apply": { "model": "miragefairy2018:building.road.side", "y": 90 }
		},
		{
			"when": { "road_south": "true" },
			"apply": { "model": "miragefairy2018:building.road.side", "y": 180 }
		},
		{
			"when": { "road_west": "true" },
			"apply": { "model": "miragefairy2018:building.road.side", "y": 270 }
		},

		{
			"apply": { "model": "miragefairy2018:building.waterway.center" }
		},
		{
			"when": { "waterway_north": "true" },
			"apply": { "model": "miragefairy2018:building.waterway.side", "y": 0 }
		},
		{
			"when": { "waterway_east": "true" },
			"apply": { "model": "miragefairy2018:building.waterway.side", "y": 90 }
		},
		{
			"when": { "waterway_south": "true" },
			"apply": { "model": "miragefairy2018:building.waterway.side", "y": 180 }
		},
		{
			"when": { "waterway_west": "true" },
			"apply": { "model": "miragefairy2018:building.waterway.side", "y": 270 }
		}

	]
}
